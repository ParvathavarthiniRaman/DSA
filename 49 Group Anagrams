class Solution:
    def groupAnagrams(self, strs: List[str]) -> List[List[str]]:
        out=defaultdict(list)
        for st in strs: 
            count=[0]*26
            for s in st :
                count[ord(s)-ord('a')]+=1
            key=tuple(count)  
            out[key].append(st)
        return list(out.values())
