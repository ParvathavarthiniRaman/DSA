class Solution:
    def searchMatrix(self, matrix: List[List[int]], target: int) -> bool:
        m=len(matrix)
        n= len(matrix[0])
        l=0
        r= (m * n) -1
        while l <=r :
            mid=(l+r) //2
            midi=mid //n
            midj=mid % n
            mid_num = matrix[midi][midj]
            if mid_num > target :
                r = mid-1
            elif mid_num < target :
                l = mid +1
            else : return True

        return False


